        <mxCell id="lTKrX1_mqVq2apyqPH1f-5" value="&lt;font color=&quot;#000000&quot;&gt;Hong Xiuquan&lt;br&gt;Taiping Rebelllion&lt;/font&gt;" style="rounded=1;whiteSpace=wrap;html=1;fontColor=#0066CC;" parent="1" vertex="1">
