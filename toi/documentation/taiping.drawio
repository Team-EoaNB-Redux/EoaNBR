        </mxCell>
        <mxCell id="3OinUYVgP6HCsZMP80dr-2" value="&lt;font color=&quot;#cc0000&quot;&gt;The Heavenly Kingdom in Flames&lt;/font&gt;&lt;br&gt;--------------------------&lt;br&gt;&lt;b style=&quot;font-style: italic;&quot;&gt;Gets Event&lt;/b&gt;&lt;i&gt;&amp;nbsp;&lt;font color=&quot;#994c00&quot;&gt;&quot;The Power Struggle&quot;&#39;&lt;/font&gt;&lt;/i&gt;&lt;i style=&quot;&quot;&gt;&lt;br&gt;&lt;/i&gt;" style="rounded=1;whiteSpace=wrap;html=1;" parent="1" vertex="1">
